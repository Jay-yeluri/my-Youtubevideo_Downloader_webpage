{
    "manifest_version": 3,
    "name": "YouTube Video Downloader",
    "version": "1.0",
    "description": "Download YouTube videos using FastAPI backend.",
    "action": {
      "default_popup": "front.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://127.0.0.1/*"  
    ]
  }
  